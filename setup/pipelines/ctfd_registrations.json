{
  "description": "Parsing for ctfd registration logs",
  "processors": [
    {
      "grok": {
        "field": "message",
        "patterns": [
          "^\\[%{DATESTAMP:datestamp}\\] %{IP:source} - %{DATA:ctfd_user} registered with %{DATA:email}$"
        ]
      }
    },
    {
      "set": {
        "field": "event.dataset",
        "value": "ctfd.registrations"
      }
    }
  ],
  "on_failure": [
    {
      "set": {
        "field": "error.message",
        "value": "{{ _ingest.on_failure_message }}"
      }
    }
  ]
}